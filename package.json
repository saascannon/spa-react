{
  "name": "@saascannon/spa-react",
  "version": "0.0.4",
  "description": "React wrapper for @saascannon/spa",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Saascannon",
    "email": "support@saascannon.com",
    "url": "https://saascannon.com"
  },
  "type": "module",
  "scripts": {
    "gen-package-lock": "npm i --package-lock-only --workspaces false",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@saascannon/spa": "^0.0.9"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "keywords": [
    "saascannon",
    "saas",
    "spa",
    "single page application",
    "react"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/saascannon/spa-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saascannon/spa-react.git"
  }
}